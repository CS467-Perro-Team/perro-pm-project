<html>
	<head>
		<title>mypmwork</title>
		<!-- Latest compiled and minified Bootstrap CSS -->
		<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.5.3/dist/css/bootstrap.min.css" integrity="sha384-TX8t27EcRE3e/ihU7zmQxVncDAy5uIKz4rEkgIXeMed4M0jlfIDPvg6uqKI2xXr2" crossorigin="anonymous">
		<!-- jQuery library -->
		<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
		<!-- Latest compiled Bootstrap JavaScript -->
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
		<style>
			html, body{
				height: 100%;
				margin: 0;
			}
			footer {
				height: 50px;
			}
			h1 {
				font-size: 24px !important;
			}
			.mainView {
				min-height: 100%;
				margin-bottom: -50px;
			}
			.signup-container{
				min-height: 75%;
				margin-bottom: 0px;
			}
			.internal-top-row, .internal-email-row {
				min-height: 25%
			}
			td { border-right: 1px solid black !important; height: 35px !important;}
		</style>
		<meta name="robots" content="noindex nofollow">
		<meta name="google-signin-scope" content="profile email">
    	<meta name="google-signin-client_id" content="YOUR_CLIENT_ID.apps.googleusercontent.com">
    	<script src="https://apis.google.com/js/platform.js" async defer></script>
  	</head>

  	<!-- Begin Body -->
    <body class="container">
    	<!-- Begin mainView-->
    	<div class="container mainView">
    		<div class="row empty-row mb-4">
    		</div>
    		<!-- Begin signup row-->
    		<div class="row signup-container mt-4">
    			<!-- Begin empty left col-->
	    		<div class="col empty-col">
	    		</div>
				<!-- End empty left col-->

    			<!-- Begin signup-box container-->
				<div class="signup-box col-4 border barder-dark">

						<!-- Begin signup top row -->
						<div class="row internal-top-row">
						</div>
						<!-- End internal login top row -->

						<!-- Begin logo row -->
						<div class="row">
							<div class="col-1">
							</div>
							<div class="col">
								<h1 class="text-center"><a href="/">mypmwork.com</a></h1>
							</div>
						</div>
						<!-- End logo row-->
						<div class="row mt-2 mb-2">
							<form class="col ml-2 mb-4">
								<div class="form-group">
									<label for="inputFirstName">First Name</label>
									<input type="text" class="form-control" id="inputFirstName" placeholder="Enter First Name" required>
								</div>
								<div class="form-group">
									<label for="inputLastName">Last Name</label>
									<input type="text" class="form-control" id="inputLastName" placeholder="Enter Last Name" required>
								</div>
							  	<div class="form-group">
							    	<label for="inputEmail">Gmail address</label>
							    	<input type="email" class="form-control" id="inputEmail" aria-describedby="emailHelp" placeholder="Enter email" required>
								</div>
							  	<button type="submit" class="btn btn-primary" name="signUp">Sign Up</button>
							</form>
						</div>
						<div class="row mb-0 pt-4">
							<div class="col-1">
							</div>
							<div class="col">
								<p class="text-center" style="font-size:12">Already have an account? <a href='/'>Sign in</a></p>
							</div>
						</div>
				</div>
				<!-- End signup-box container -->
				<!-- Begin empty right col-->
				<div class="col empty-col">
				</div>
				<!-- End empty right col-->
    		</div>		
			<!-- End signup container-->
		</div>
		<!-- End mainView -->
		<!-- Begin Footer -->
	    <footer class="footer">
	    	<!-- Begin Footer Row -->
	    	<div class="row">
	    		<!-- Begin empty left column -->
	    		<div class="col">
	    		</div>
	    		<!-- End empty left column -->

	    		<!-- Begin Middle column -->
				<div class="col-6">
					<ul class="nav justify-content-center">
						<li class="nav-item"><a class="nav-link" href="/">mypmwork.com</a></li>
						<li class="nav-item"><a class="nav-link" href="/">help</a></li>
						<li class="nav-item"><a class="nav-link" href="/">resources</a></li>
						<li class="nav-item"><a class="nav-link" href="/siteMap">about</a></li>
						<li class="nav-item"><a class="nav-link" href="#" onclick="signOut();">sign out</a></li>
					</ul>
	    		</div>
	    		<!-- End Middle column -->

	    		<div class="col">
	    		</div>
	    		<!-- End empty right column -->
	    	</div>
	    	<!-- End Footer Row-->
	    </footer>
	    <!-- End Footer -->
    </body>
  	<!-- End Body -->

</html>